<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#56A8E7"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.gigamole.library.PulseView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/pulseView"
        app:pv_icon="@drawable/ic_people_black_24dp"
        app:pv_alpha="70"
        app:pv_color="#FFF"
        app:pv_count="8"
        app:pv_measure="height"
        app:pv_icon_width="26dp"
        app:pv_icon_height="36dp"
        app:pv_spawn_period="700"
        app:pv_interpolator="@android:anim/linear_interpolator"/>

    <app.m26.wikidriver.custom.CustomFloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:src="@drawable/w_icon"
        android:visibility="gone"
        fab:fab_showShadow="false"
        fab:elevation="4dp"
        android:id="@+id/fbWidget"/>
</RelativeLayout>